<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="MainPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.controllers.MainController">
   <children>
      <AnchorPane fx:id="TOP_BAR" prefHeight="50.0" prefWidth="600.0">
         <children>
            <Button fx:id="CLOSE_BUTTON" maxHeight="10.0" maxWidth="30.0" minHeight="20.0" minWidth="30.0" mnemonicParsing="false" prefHeight="10.0" prefWidth="30.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <Button fx:id="COLAPSE_BUTTON" minHeight="20.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="20.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
      <AnchorPane prefHeight="350.0" prefWidth="50.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
         <children>
            <VBox prefWidth="50.0">
               <children>
                  <Button fx:id="showLoginLogButton" mnemonicParsing="false" onAction="#onClickShowLoginLog" prefHeight="50.0" prefWidth="50.0" text="L" />
                  <Button fx:id="showGameLogButton" mnemonicParsing="false" onAction="#onClickShowGameLog" prefHeight="50.0" prefWidth="50.0" text="G" />
               </children>
            </VBox>
         </children>
      </AnchorPane>
      <Button layoutX="92.0" layoutY="95.0" mnemonicParsing="false" onAction="#startApp1" text="Button" />
      <Button layoutX="166.0" layoutY="95.0" mnemonicParsing="false" onAction="#startApp2" text="Button" />
   </children>
</AnchorPane>
